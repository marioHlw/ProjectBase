package ActionData;

message LAddShock{
    optional bool onceEvent = 1;
    optional int32 linkId = 2;
    optional float power = 3 [default=2];
    optional float angle = 4 [default=0];
    optional float time = 5 [default=1];
    optional int32 frequency = 6 [default=1];
    optional float decay = 7 [default=0.98];
    optional bool randomAngle = 8;
}


    
    